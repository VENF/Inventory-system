{
  "name": "inventory-system",
  "version": "1.0.0",
  "description": "inventory system",
  "main": "index.js",
  "scripts": {
    "lint": "prettier --write ./src/**/*.{ts,json,md} ./_tests_/**/*.ts",
    "start": "cross-env NODE_ENV=pro node ./dist/index.js",
    "dev": "rimraf dist && cross-env NODE_ENV=dev tsc-watch --onSuccess \"node ./dist/index.js\"",
    "build": "cross-env NODE_ENV=pro tsc",
    "clean": "rimraf ./dist",
    "tests": "cross-env NODE_ENV=test npx jest --detectOpenHandles",
    "tw": "cross-env NODE_ENV=test npx jest --watchAll --detectOpenHandles",
    "testsc": "cross-env NODE_ENV=test npx jest --coverage --detectOpenHandles",
    "start-client": "cd client && yarn start"
  },
  "keywords": [
    "inventory",
    "system",
    "node",
    "react",
    "clothes"
  ],
  "author": "Victor Naranjo & Raul Chirinos",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.0",
    "@types/mongoose": "^5.7.23",
    "@types/morgan": "^1.9.0",
    "@types/supertest": "^2.0.9",
    "jest": "^26.0.1",
    "node-sass": "^7.0.0",
    "prettier": "2.0.5",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "ts-watch": "^1.0.8",
    "tsc-watch": "^4.2.8",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@types/uuid": "^8.0.0",
    "add": "^2.0.6",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.1",
    "supertest": "^4.0.2",
    "uuid": "^8.2.0",
    "yarn": "^1.22.4"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
